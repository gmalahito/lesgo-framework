{
  "name": "lesgo",
  "version": "1.1.0",
  "description": "Core framework for lesgo node.js serverless framework.",
  "main": "./src/index.js",
  "author": "Sufiyan Rahmat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reflex-media/lesgo-framework/issues"
  },
  "homepage": "https://github.com/reflex-media/lesgo-framework#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reflex-media/lesgo-framework.git"
  },
  "files": [
    "src",
    "bin"
  ],
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run test",
    "lint": "eslint '**/*.{js,ts,jsx,tsx}' --quiet",
    "lint-fix": "eslint '**/*.{js,ts,jsx,tsx}' --fix"
  },
  "bin": {
    "lesgo-scripts": "./bin/lesgo-scripts.sh"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "aws-sdk": "^2.1345.0",
    "babel-jest": "^29.5.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.13.0",
    "@firebase/app": "^0.7.33",
    "data-api-client": "^1.3.0",
    "firebase-admin": "^11.5.0",
    "jsonwebtoken": "^9.0.0",
    "memcache-plus": "^0.2.24",
    "mysql2": "^2.3.3",
    "nanoid": "^3.3.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "npm run lint",
      "git add"
    ]
  }
}
